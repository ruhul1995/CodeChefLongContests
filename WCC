import java.util.*

fun main(args:Array<String>) {
    var input = Scanner(System.`in`)
    var testcase = input.nextInt()

    for (i in 1..testcase) {
        var X = input.nextInt()
        var str = input.next()

        var carlPoints = 0
        var chefPoints = 0
        var carlTotal = 0
        var chefTotal = 0
        for(i in str.indices)
        {
            if(str[i] == 'C')
                carlPoints +=2
            else if (str[i] == 'N')
                chefPoints += 2
            else if(str[i] == 'D')
            {
                carlPoints += 1
                chefPoints += 1
            }
        }
        //println("carlsen point = $carlPoints")
        //println("chefPoints point = $chefTotal")
        if(carlPoints > chefPoints)
        {
            //carlsen wins
            carlTotal = 60 * X
            //chefTotal = 40 * X
            println(carlTotal)
        }
        else if(carlPoints == chefPoints)
        {
            //draw
            carlTotal = 55 * X
            //chefTotal = 45 * X
            println(carlTotal)
        }
        else
        {
            //chef wins
            //chefTotal =  60 * X
            carlTotal = 40 * X
            println(carlTotal)
        }
    }
}

